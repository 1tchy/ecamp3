
<tal:block condition="login">
	<h2>Currently Authenticated as:</h2>

	<p><tal:block content="login/getLogin"/></p>
</tal:block>


<h2>Login as:</h2>
<table>
	<tr tal:repeat="login logins">
		<td><a tal:attributes="href '/login/login/${login/getId}'">Login</a></td>
		<td>::</td>
		<td tal:content="login/getLogin" />
	</tr>
</table>

